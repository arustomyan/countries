const arr = [
    'fff',
    'errgeg',
    'fffffffffff'
]

console.log(arr.filter((item => {
    const search = 'ff'

    for (let index = 0; index < search.length; index++) {
        console.log(`${item[index]} !== ${search[index]}`)
        if (item[index] !== search[index]) {
            
            return false
        } 
    }

    return true

})))

